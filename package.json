{
  "name": "nyt-react-scraper",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "babel-node": "babel-node --presets=env",
    "start": "node ./bin/www",
    "client": "cd client && yarn start",
    "server": "nodemon --exec npm run babel-node -- ./bin/www --ignore client",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\""
  },
  "dependencies": {
    "app-root-path": "^2.1.0",
    "axios": "^0.18.0",
    "babel-preset-env": "^1.7.0",
    "bootstrap": "^4.1.3",
    "cheerio": "^1.0.0-rc.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "lodash": "^4.17.11",
    "mongoose": "^5.2.16",
    "morgan": "~1.9.0",
    "react-popper": "^1.0.2",
    "react-router-dom": "^4.3.1",
    "reactstrap": "^6.4.0",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "concurrently": "^4.0.1",
    "eslint": "^5.6.0"
  }
}
